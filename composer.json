{
    "name": "apsonex/saas-theme",
    "authors": [
        {
            "name": "Apsonex Inc.",
            "email": "apsonexinc@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Apsonex\\SaasTheme\\": "src",
            "Apsonex\\SaasTheme\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Apsonex\\SaasTheme\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "fakerphp/faker": "^1.20",
        "orchestra/testbench": "^7.15",
        "phpunit/phpunit": "^9.5"
    }
}
